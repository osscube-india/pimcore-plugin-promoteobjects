<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
		<plugin>

            <!-- the plugin name [A-Za-z_] -->
            <pluginName>PromoteObjects</pluginName>

			<pluginNiceName>Promote objects from source to target environment</pluginNiceName>
            <!-- a short description -->
			<pluginDescription><![CDATA[]]></pluginDescription>

            <!-- meta data -->
            <pluginVersion>1.0</pluginVersion>
            <pluginRevision>1</pluginRevision>
            <pluginBuildTimestamp>0</pluginBuildTimestamp>

            <!-- put in the URL for the plugin configuration eg. /plugin/PromoteObjects/admin/index -->
            <pluginIframeSrc></pluginIframeSrc>

            <!-- className of the plugin which extends Pimcore_API_Plugin_Abstract-->
			<pluginClassName>\PromoteObjects\Plugin</pluginClassName>

            <!-- include paths relative to plugin-directory -->
			<pluginIncludePaths>
				<path>/PromoteObjects/lib</path>
			</pluginIncludePaths>

			<!-- namespaces to register with autoloader-->
			<pluginNamespaces>
				<namespace>PromoteObjects</namespace>
			</pluginNamespaces>

			<!-- js files with path relative to plugin-directory -->
			<pluginJsPaths>
                <path>/plugins/PromoteObjects/static/js/startup.js</path>
			</pluginJsPaths>

			<!-- css files with path relative to plugin-directory -->
			<pluginCssPaths>
                <path>/plugins/PromoteObjects/static/css/example.css</path>
			</pluginCssPaths>
			
			<!-- Logger Type-->
			<logger>
				<type>
					<CRIT>true</CRIT>
					<ERROR>true</ERROR>
					<WARN>true</WARN>
					<INFO>false</INFO>
					<ALERT>false</ALERT>
					<DEBUG>false</DEBUG>
				</type>
				<filename>promote-objects-logs{-date}.log</filename>
			</logger>
		</plugin>
</zend-config>
